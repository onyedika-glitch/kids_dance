#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/nuxt@3.12.2_@opentelemetry+api@1.9.0_@parcel+watcher@2.4.1_@types+node@20.14.9_encoding@0.1.1_uylnhjfsm4b52y634edwamexvi/node_modules/nuxt/bin/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/nuxt@3.12.2_@opentelemetry+api@1.9.0_@parcel+watcher@2.4.1_@types+node@20.14.9_encoding@0.1.1_uylnhjfsm4b52y634edwamexvi/node_modules/nuxt/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/nuxt@3.12.2_@opentelemetry+api@1.9.0_@parcel+watcher@2.4.1_@types+node@20.14.9_encoding@0.1.1_uylnhjfsm4b52y634edwamexvi/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/nuxt@3.12.2_@opentelemetry+api@1.9.0_@parcel+watcher@2.4.1_@types+node@20.14.9_encoding@0.1.1_uylnhjfsm4b52y634edwamexvi/node_modules/nuxt/bin/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/nuxt@3.12.2_@opentelemetry+api@1.9.0_@parcel+watcher@2.4.1_@types+node@20.14.9_encoding@0.1.1_uylnhjfsm4b52y634edwamexvi/node_modules/nuxt/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/nuxt@3.12.2_@opentelemetry+api@1.9.0_@parcel+watcher@2.4.1_@types+node@20.14.9_encoding@0.1.1_uylnhjfsm4b52y634edwamexvi/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
