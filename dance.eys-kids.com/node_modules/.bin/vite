#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/vite@6.0.11_@types+node@20.14.9_jiti@2.4.0_sass@1.80.6_terser@5.31.1_yaml@2.4.5/node_modules/vite/bin/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/vite@6.0.11_@types+node@20.14.9_jiti@2.4.0_sass@1.80.6_terser@5.31.1_yaml@2.4.5/node_modules/vite/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/vite@6.0.11_@types+node@20.14.9_jiti@2.4.0_sass@1.80.6_terser@5.31.1_yaml@2.4.5/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/vite@6.0.11_@types+node@20.14.9_jiti@2.4.0_sass@1.80.6_terser@5.31.1_yaml@2.4.5/node_modules/vite/bin/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/vite@6.0.11_@types+node@20.14.9_jiti@2.4.0_sass@1.80.6_terser@5.31.1_yaml@2.4.5/node_modules/vite/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/vite@6.0.11_@types+node@20.14.9_jiti@2.4.0_sass@1.80.6_terser@5.31.1_yaml@2.4.5/node_modules:/mnt/c/sc_monorepo_cloned/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
